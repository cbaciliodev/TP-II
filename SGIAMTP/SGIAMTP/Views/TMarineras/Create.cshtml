@model SGIAMTP.Models.TMarinera

@{
    ViewData["Title"] = "Create";
}

<h1>Ingresar Marinera</h1>

<h4>TusuyPeru</h4>
<hr />
<section>

    <form asp-action="Create">
        <div asp-validation-summary="ModelOnly" class="text-danger"></div>
        <div class="form-row">
            <div class="form-group col-md-6">
                <label asp-for="PkImCod" class="control-label">Codigo Canción</label>
                <input asp-for="PkImCod" class="form-control" placeholder="Codigo Canción" />
                <span asp-validation-for="PkImCod" class="text-danger"></span>
            </div>
            <div class="form-group col-md-6">
                <label asp-for="VmNombre" class="control-label">Nombre de la Canción</label>
                <input asp-for="VmNombre" class="form-control" placeholder="Nombre de la Canción" />
                <span asp-validation-for="VmNombre" class="text-danger"></span>
            </div>
        </div>
        <div class="form-group">
            <a class="btn btn-warning" asp-action="Index">Regresar</a>
            <input type="submit" value="Registrar" class="btn btn-primary" />

        </div>
    </form>
    <div></div>

</section>


@section Scripts {
    @{await Html.RenderPartialAsync("_ValidationScriptsPartial");}
}
